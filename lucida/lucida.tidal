do
  -- @name drums
  d1 $ "0 . ~ 0 ~@2"
  d1 $ "[0@3 0]*2"
  d1 $ "0 ~ ~ 0? ~ ~ 0? ~ . ~ 0 ~ [~ 0]"
  d2 $ "[~ 0]*2"
  d8 $ "0*16"

do
  -- @name full
  setF "plink" 0
  setcps (90/120/2)
  d1 $ "[0@3 0]*2"
  -- d1 $ "0 ~ ~ 0? ~ ~ 0? ~ . ~ 0 ~ [~ 0]"
  d2 $ "[~ 0]*2"
  d8 $ "0*16"
  d1 $ "0  ~ ~ 0? ~@4. [~ 0] 0 ~ [~ 0]"
  d3 $ fast 2 $ n "~ 0 0 ~"
  s2
    $ fast 2
    $ sometimesBy "^plink" (# (octave (5 |+ irand 3) # sus 4))
    $ (note . cat)
    -- $ (note . cat . fmap (k 0) . fmap (rarely (|+ ("[1 0@3]*4" |* (irand 8) |- 4))))
    (
      fmap (k 0) [
      "2 4 5 [2|-1] 4 0 4 5"
    , "6 8 9 6 8 -4 -2 -1"
    , "1 2 3 1 2 -1 1 2"
    ] ++ fmap (k 1) [
      "[0 1 2 0 1 -2 1 2 | 0 1 2 3 6 5 3 2]"
    ])
    # sus 1.4
    # amp 0.5
  s3
    $ fast 2
    $ (note . cat)
      ([ k 0 "0(3,8)"
      , k 0 "[~@2 0 ~@2 0 ~@2 | [0 ~] [-3 ~] [0 ~] ~]"
      , k 0 "0(3,8)" |+ 1
      , k 1 "~@2 0 ~@2 4 ~@2"
      ]
    )
    # octave 3
    # sus 0.1
    # amp 0.8

do
  -- @name steal
  d1 $ "0  ~ ~ 0? ~@4. [~ 0] 0 ~ [~ 0]"
  d2 $ stack ["[~ [0@3 0??]]*2" #Â amp 0.45, degrade "0*8" # amp (range 0.1 0.35 $ rand)]
  s1 $ fast 2
    $ stack [
      -- midibend $ (slow 8 $ struct "t*128" $ (range 0 8192 $ sine)),
      (note . cat)
      ([ k 0 $ "~ 6 9 5 . 8 6 4 2"
       , k 0 $ "~ 1 ~ [6, ~@2 3@3,~@3 1@2]@5"
       -- , k 0 $ "<[~ 0 ~ [5, ~@2 2@3,~@3 0@2]@5] [~ 2 ~ 4 ~ 5 ~ 6]>"
       -- , k 1 $ "<[~ 0 ~ [5, ~@2 2@3,~@3 0@2]@5] [~ 2 ~ 4 ~ 5 ~ 6]>"
       , k 0 $ "[~ 0 ~ [5, ~@2 2@3,~@3 0@2]@5]"
       , k 1 $ "[~ 0 ~ [5, ~@2 2@3,~@3 -1@2]@5]" |- 4
       ])
    # amp 0.4
    # sus 2]

do
  -- @name end
  d2 silence
  d8 silence
  setF "plink" 0.3