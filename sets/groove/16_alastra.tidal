  setI "chop" "[1 [2|8|4]!7]*2"
  all $ byshuf (shuffle "^shuf") . byacc (chop "^chop" . (|* amp 0.3))

do
  setkey' 2 "0" "lydian"
  keys $ ((0.01 <~) . stack)
    [
      ((1/8) <~)
      $ cat [ 
        nok 1 $ "2@4 2 ~ ~ ~ " |+ "[-2,0,2,4]",
        nok 2 $ "2@4 2 ~ ~ ~ " |+ "[-2,0,2,4]",
        nok 0 $ "0@4 0 ~ ~ ~ " |+ "[-3,0,2,4,6]",
        nok 0 $ "0 ~ 1 ~ 2 ~ 3 ~" |+ "[-3,0,2,4]"
      ],
        ((1/8) ~>)
        $ nokc [1,2,0,0]
        [ "[6 9 5 8 . 4 6 2 5]"
        , "[1 4 0 3 . -2 1 -3 0]" |- 2
        , "[6 9 5 8 . 4 6 2 5]"
        , "[1 4 0 2 . -2 1 -3 0]"
        ]
        # amp 0.2
        # sus 0.4
        # octave 6
    ]
  -- lt
