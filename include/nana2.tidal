    ]
  p "bes" ## [s "[tb:0,tb:1]" # octave 3 # gain 0.7 # lpf 350]
    $ (|+ up 0.13)
    $ fast 2
    $ cat
      [ t "11 9 <7!2 6!2>"
      , t "[16 13 15 12 13 15,]"
      ]


do
  p "drums" $ stack
    [ n "[2 ~ 2 ~ 2 1@2 2 ~ 2 ~ 2 2 ~ 1 ~ 2 2]/1.5" # s "bhh" # gain 0.5
    , s "[bbd:0,bd:0]*4" # gain 0.8
    , n "[3 ~ 5 ~ 5 3 . 3 ~ 5 5 ~ 3 . 3 ~ 5 ~ 5 3]/1.5" # s "vs" # gain 0.75
    ]
  bes $ t "12*4"
  p "hh" $ fast 4 $ s "th*3" # gain (range 0.84 0.5 saw)

do

vibe silence

p "drums" $ silence

p "bes" ## [s "[tb:0,tb:1]" # octave 4 # lpf 300 # gain 0.9]
  $ (|+ up 0.13)
  $ fast 2
  $ cat
    [ t "11@2 6 7 9 13"
    , t ("~ 12 ~ 12@2 ~")
    , t "[16 13 15 12 13 15,]"
    ]


