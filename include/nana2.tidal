do
  -- @setup
  setkey 7 "lydian"
  setkey' 0 7 "lydian"
  setcps (112/120/2)
  all (# bus' 0.6 0.6)

let piano = p "piano" ## [s "superpiano" # velocity 0.25 # gain 1.8 # octave 3 # legato 2]
    vibe = p "vibe" ## [s "supervibe" # modamp 0 # sus 4 # velocity 0.3 # sus 4 # gain 0.7 # lpf 2000]
    bes = p "bes" ## [s "[tb:0,tb:1]" # octave 3 # gain 0.7 # lpf 350]



do
  hush
  piano
    $ fast 2
    $ cat
      [ t "[[<0!4 -2!4>,4,11]@2 6 7 9 13,]" # sus 4
      , t ("[~ 1 ~ 1@2 ~]" |+ "[0,4,6,8,9]") # sus "0.2 4" # velocity 0.1
      , t "[16 13 15 12 13 15,]" # sus 4
      -- , t "16 13 15 11 12 15"
      ]

do
  p "drums" $ stack
    [ n "[2 ~ 2 ~ 2 1@2 2 ~ 2 ~ 2 2 ~ 1 ~ 2 2]/1.5" # s "bhh" # gain 0.5
    , s "[bbd:0,bd:0]*4" # gain 0.8
    , n "[3 ~ 5 ~ 5 3 . 3 ~ 5 5 ~ 3 . 3 ~ 5 ~ 5 3]/1.5" # s "vs" # gain 0.75
    ]

  bes $ t "12*4"

  p "hh" $ fast 4 $ s "th*3" # gain (range 0.84 0.5 saw)

do
